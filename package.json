{
  "name": "giftpool",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "dev:coverage": "COVERAGE=1 nyc next dev -p 4000",
    "migrate": "supabase migration up",
    "db:deploy": "supabase db push --project-ref $SUPABASE_PROJECT_ID",
    "seed": "NEXT_PUBLIC_SUPABASE_URL=$NEXT_PUBLIC_SUPABASE_URL NEXT_PUBLIC_SUPABASE_ANON_KEY=$NEXT_PUBLIC_SUPABASE_ANON_KEY bun run src/db/seed.ts",
    "lint": "bun biome check --apply .",
    "type": "bun tsc",
    "e2e": "bun run migrate && bun run seed && playwright test",
    "e2e:ci": "bun run migrate && bun run seed && CI=true playwright test",
    "e2e:cov": "bun run migrate && bun run seed && playwright test; nyc report",
    "e2e:cov:ci": "bun run migrate && bun run seed && CI=true playwright test; nyc report",
    "build": "NODE_ENV=production next build",
    "vercel-build": "bun run db:deploy && bun run build",
    "start": "next start -p 8080",
    "prepare": "husky install"
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@fontsource/roboto": "^5.0.13",
    "@mui/icons-material": "5.15.16",
    "@mui/material": "5.15.20",
    "@sentry/nextjs": "^7.113.0",
    "@supabase/supabase-js": "^2.43.0",
    "@types/react": "18.3.1",
    "@types/react-copy-to-clipboard": "^5.0.7",
    "@types/react-dom": "18.3.0",
    "autoprefixer": "10.4.19",
    "better-supabase-types": "^2.7.5",
    "next": "14.2",
    "next-pwa": "^5.6.0",
    "pino": "^9.0.0",
    "postcss": "8.4.38",
    "postgres": "^3.4.4",
    "react": "canary",
    "react-copy-to-clipboard": "^5.1.0",
    "react-dom": "canary",
    "react-share": "^5.1.0",
    "react-toastify": "^10.0.5",
    "tailwindcss": "3.4.3",
    "typescript": "5.4.5",
    "zod": "^3.23.6"
  },
  "devDependencies": {
    "@biomejs/biome": "1.7.2",
    "@happy-dom/global-registrator": "^14.7.1",
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@playwright/test": "1.43.1",
    "@testing-library/react": "^15.0.6",
    "@testing-library/user-event": "^14.5.2",
    "@types/bun": "^1.1.1",
    "babel-loader": "^9.1.3",
    "babel-plugin-istanbul": "^6.1.1",
    "dotenv-cli": "^7.4.1",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "nyc": "^15.1.0",
    "playwright-test-coverage": "^1.2.12",
    "webpack": "^5.91.0"
  }
}
